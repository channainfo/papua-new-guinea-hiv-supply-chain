<%= simple_form_for [:admin , @lab_test],:html => { :class => 'form-horizontal' } do |f| %>
  <%= f.error_notification %> 
  <%= f.input :name %>

  <%= f.association :lab_test_category, collection: LabTestCategory.all, :include_blank => 'Select Lab Test Category' %>
  <%= f.association :unit, collection: Unit.all, :include_blank => 'Select Unit' %>

  <div class="form-actions">
    <%= button_save ' Save ' %>
    <%= link_button_cancel 'Cancel', admin_lab_tests_path() %>
  </div>
<% end %>
