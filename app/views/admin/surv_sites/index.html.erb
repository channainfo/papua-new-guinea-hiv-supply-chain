<% content_for :breadcrumb do %>
  <%= breadcrumb [{"Surv Sites" => false}] %>
<% end %>

<%= page_header "Surv Sites : " + @import_surv.surv_type  %>

<table class="table table-hover ">
  <thead>
    <tr>
      <th class='nth-table'> # </th>
      <th> Site </th>
      <th> Year </th>
      <th> Month </th>
      <th> Type </th>
      <th> Total </th>
    </tr>
  </thead>
  <tbody>
    <% @surv_sites.each_with_index do |surv_site, index| %>
      <tr>
        <td><%=current_entries + index %></td>
        <td>
           <%= link_button_edit surv_site.site.name, admin_surv_site_surv_site_commodities_path(surv_site) %>
        </td>
        <td><%=surv_site.year %></td>
        <td><%=surv_site.month %></td>
        <td><%=surv_site.surv_type %></td>
        <td><%=surv_site.surv_site_commodities_count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= render_paginate_for @surv_sites %>